  <rdf:Description rdf:about="https://www.omg.org/spec/Commons/QuantitiesAndUnits/SystemOfQuantities">
    <rdfs:subClassOf rdf:nodeID="Na89f63400b5549dcbcf9143e8198efdd"/>
    <rdfs:subClassOf rdf:nodeID="Nb1050b1846714641a0993631abb720fa"/>
