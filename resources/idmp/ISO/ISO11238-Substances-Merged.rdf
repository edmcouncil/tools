  <rdf:Description rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/MechanicalIngredient">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
