    <cmns-txt:hasTextValue>Specified Substance Group 1</cmns-txt:hasTextValue>
  </rdf:Description>
  <rdf:Description rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO21090-HarmonizedDatatypes/hasMediaType">
