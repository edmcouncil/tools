  <rdf:Description rdf:about="https://www.omg.org/spec/Commons/QuantitiesAndUnits/QuantityKind">
    <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/Commons/Classifiers/Classifier"/>
    <rdfs:subClassOf rdf:nodeID="Nd635d08980df4ef78dfca9b07fe7370a"/>
