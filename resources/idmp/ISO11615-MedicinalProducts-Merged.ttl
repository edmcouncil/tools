                                                                                       ] ,
                                                                                       [ rdf:type owl:Restriction ;
                                                                                         owl:onProperty <https://www.omg.org/spec/Commons/QuantitiesAndUnits/hasMinimumPermissiveValue> ;
                                                                                         owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
