		owl:DatatypeProperty ,
		owl:FunctionalProperty
		;
	rdfs:subPropertyOf idmp-dtp:hasIntegerValue ;
	rdfs:label "has number of moieties" ;
	dct:source "ISO/TS 19844:2018(E) Health informatics - Identification of medicinal products (IDMP) - Implementation guidelines for ISO 11238 for data elements and structures for the unique identification and exchange of regulated information on substances, clause 7.1.4" ;
	rdfs:domain idmp-sub:ChemicalSubstance ;
