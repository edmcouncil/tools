        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/Commons/PartiesAndSituations/"/>
        <rdfs:label>undergoes</rdfs:label>
        <skos:definition>indicates a situation that the undergoer experiences</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.omg.org/spec/Commons/ProductsAndServices/isIssuedBy -->


    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/Commons/ProductsAndServices/isIssuedBy">
