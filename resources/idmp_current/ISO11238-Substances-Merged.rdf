    </owl:ObjectProperty>
    


    <!-- https://www.omg.org/spec/Commons/PartiesAndSituations/playsActiveRoleIn -->


    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/Commons/PartiesAndSituations/playsActiveRoleIn">
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://www.omg.org/spec/Commons/PartiesAndSituations/playsRole"/>
            <rdf:Description rdf:about="https://www.omg.org/spec/Commons/PartiesAndSituations/isSubjectRoleIn"/>
        </owl:propertyChainAxiom>
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/Commons/PartiesAndSituations/"/>
        <rdfs:label>plays active role in</rdfs:label>
        <skos:definition>relates something to a situation that it is directly involved in</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.omg.org/spec/Commons/PartiesAndSituations/playsActiveRoleThatAffects -->


    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/Commons/PartiesAndSituations/playsActiveRoleThatAffects">
