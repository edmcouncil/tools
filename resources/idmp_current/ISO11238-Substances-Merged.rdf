        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.omg.org/spec/Commons/QuantitiesAndUnits/QuantityValue"/>
                    <rdf:Description rdf:about="https://www.omg.org/spec/Commons/QuantitiesAndUnits/QuantityValueRange"/>
