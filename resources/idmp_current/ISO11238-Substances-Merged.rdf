    <!-- https://www.omg.org/spec/Commons/QuantitiesAndUnits/expressesTheMagnitudeOf -->


    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/Commons/QuantitiesAndUnits/expressesTheMagnitudeOf">
        <rdfs:range rdf:resource="https://www.omg.org/spec/Commons/QuantitiesAndUnits/Quantity"/>
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/Commons/QuantitiesAndUnits/"/>
