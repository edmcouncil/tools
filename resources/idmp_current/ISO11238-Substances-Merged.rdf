    </owl:ObjectProperty>
    


    <!-- https://www.omg.org/spec/Commons/PartiesAndSituations/playsRole -->


    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/Commons/PartiesAndSituations/playsRole">
