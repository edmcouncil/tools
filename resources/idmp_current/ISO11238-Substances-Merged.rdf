        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="https://www.omg.org/spec/Commons/QuantitiesAndUnits/QuantityValue"/>
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/Commons/QuantitiesAndUnits/"/>
        <rdfs:label>has lower bound</rdfs:label>
        <skos:definition>specifies the quantity value that is the lower value of a pair of values representing a range</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.omg.org/spec/Commons/QuantitiesAndUnits/hasMaximumPermissiveValue -->


    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/Commons/QuantitiesAndUnits/hasMaximumPermissiveValue">
