    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/Commons/ProductsAndServices/situates">
        <owl:inverseOf rdf:resource="https://www.omg.org/spec/Commons/ProductsAndServices/isSituatedAt"/>
