            <rdf:Description rdf:about="https://www.omg.org/spec/Commons/PartiesAndSituations/actsIn"/>
            <rdf:Description rdf:about="https://www.omg.org/spec/Commons/PartiesAndSituations/hasUndergoer"/>
            <rdf:Description rdf:about="https://www.omg.org/spec/Commons/PartiesAndSituations/isPlayedBy"/>
        </owl:propertyChainAxiom>
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/Commons/PartiesAndSituations/"/>
