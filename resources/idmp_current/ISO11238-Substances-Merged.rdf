            <rdf:Description rdf:about="https://www.omg.org/spec/Commons/PartiesAndSituations/isObjectRoleIn"/>
        </owl:propertyChainAxiom>
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/Commons/PartiesAndSituations/"/>
