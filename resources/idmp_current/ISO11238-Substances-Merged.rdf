                <owl:allValuesFrom rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/MolecularFormula"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
