                <owl:onProperty rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/hasDefiningMolecularFormulaByMoiety"/>
                <owl:allValuesFrom rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/MolecularFormulaByMoiety"/>
            </owl:Restriction>
        </rdfs:subClassOf>
