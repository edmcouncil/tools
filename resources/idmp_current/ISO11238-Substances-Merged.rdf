        <rdfs:label>object identifier</rdfs:label>
        <skos:definition>globally unique identifier of a data object as defined in ISO/IEC 8824-2</skos:definition>
        <AnnotationVocabulary:abbreviation>OID</AnnotationVocabulary:abbreviation>
        <AnnotationVocabulary:adaptedFrom>ISO 21090:2011 Health informatics - Harmonized data types for information interchange, clause 4</AnnotationVocabulary:adaptedFrom>
        <AnnotationVocabulary:explanatoryNote>An object identifier (OID) is a unique alphanumeric/numeric identifier registered under the ISO/IEC 8824-2 registration standard to reference a specific object or object class.</AnnotationVocabulary:explanatoryNote>
    </owl:Class>
    


    <!-- https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO21090-HarmonizedDatatypes/UpdateMode -->


    <owl:Class rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO21090-HarmonizedDatatypes/UpdateMode">
        <rdfs:subClassOf rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO21090-HarmonizedDatatypes/DatatypeFlavor"/>
