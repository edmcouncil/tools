    <!-- https://www.omg.org/spec/Commons/QuantitiesAndUnits/hasMinimumPermissiveValue -->


    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/Commons/QuantitiesAndUnits/hasMinimumPermissiveValue">
        <rdfs:subPropertyOf rdf:resource="https://www.omg.org/spec/Commons/QuantitiesAndUnits/hasQuantityValue"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://www.omg.org/spec/Commons/QuantitiesAndUnits/MeasurementScale"/>
        <rdfs:range rdf:resource="https://www.omg.org/spec/Commons/QuantitiesAndUnits/QuantityValue"/>
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/Commons/QuantitiesAndUnits/"/>
        <rdfs:label>has minimum permissive value</rdfs:label>
        <skos:definition>indicates the minimum allowed value for a measurement on the given scale</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.omg.org/spec/Commons/QuantitiesAndUnits/hasNumerator -->


    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/Commons/QuantitiesAndUnits/hasNumerator">
