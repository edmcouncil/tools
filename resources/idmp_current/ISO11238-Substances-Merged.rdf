        <AnnotationVocabulary:adaptedFrom>ISO 1087 Terminology work and terminology science - Vocabulary, Second edition, 2019-09</AnnotationVocabulary:adaptedFrom>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#altLabel"/>
    </owl:AnnotationProperty>
    


    <!-- https://www.omg.org/spec/Commons/AnnotationVocabulary/usageNote -->


    <owl:AnnotationProperty rdf:about="https://www.omg.org/spec/Commons/AnnotationVocabulary/usageNote">
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/Commons/AnnotationVocabulary/"/>
        <rdfs:label>usage note</rdfs:label>
        <skos:definition>note that provides information about how a given resource is used or may be extended</skos:definition>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#note"/>
    </owl:AnnotationProperty>
    


