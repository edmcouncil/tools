    <!-- https://www.omg.org/spec/Commons/ProductsAndServices/isProvidedBy -->


    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/Commons/ProductsAndServices/isProvidedBy">
        <owl:inverseOf rdf:resource="https://www.omg.org/spec/Commons/ProductsAndServices/provides"/>
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/Commons/ProductsAndServices/"/>
