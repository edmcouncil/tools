    </owl:ObjectProperty>
    


    <!-- https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/isGranularPartOf -->


    <owl:ObjectProperty rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/isGranularPartOf">
