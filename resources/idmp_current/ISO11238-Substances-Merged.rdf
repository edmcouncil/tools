        <AnnotationVocabulary:explanatoryNote>Something may be situated at some site, or in some setting, situation, or context.</AnnotationVocabulary:explanatoryNote>
    </owl:ObjectProperty>
    


    <!-- https://www.omg.org/spec/Commons/ProductsAndServices/issues -->


    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/Commons/ProductsAndServices/issues">
        <rdfs:subPropertyOf rdf:resource="https://www.omg.org/spec/Commons/ProductsAndServices/provides"/>
