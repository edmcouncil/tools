        <rdfs:subClassOf rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO21090-HarmonizedDatatypes/ISO21090-AnyDatatype"/>
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/Commons/ContextualDesignators/ContextualName"/>
        <terms:source>ISO 21090:2011 Health informatics - Harmonized data types for information interchange, clause 7.7.6</terms:source>
