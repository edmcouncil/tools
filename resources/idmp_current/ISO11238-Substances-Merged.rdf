        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/Commons/QuantitiesAndUnits/"/>
        <rdfs:label>has expression</rdfs:label>
        <skos:definition>specifies a finite combination of symbols, including constants, variables that may be ordered via one or more operators, that is well-formed according to rules that depend on the language and context</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.omg.org/spec/Commons/QuantitiesAndUnits/hasFactor -->


    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/Commons/QuantitiesAndUnits/hasFactor">
