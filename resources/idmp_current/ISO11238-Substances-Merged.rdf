        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/"/>
        <rdfs:label>has maximum longitude</rdfs:label>
        <skos:definition>a maximum longitude with respect to a range</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- https://www.omg.org/spec/LCC/Countries/CountryRepresentation/hasMinimumLatitude -->


    <owl:DatatypeProperty rdf:about="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/hasMinimumLatitude">
        <rdfs:subPropertyOf rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/hasLatitude"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/"/>
        <rdfs:label>has minimum latitude</rdfs:label>
        <skos:definition>a minimum latitude with respect to a range</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- https://www.omg.org/spec/LCC/Countries/CountryRepresentation/hasMinimumLongitude -->


