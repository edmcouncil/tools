    <!-- https://www.omg.org/spec/Commons/PartiesAndSituations/manifests -->


    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/Commons/PartiesAndSituations/manifests">
        <rdfs:range rdf:resource="https://www.omg.org/spec/Commons/PartiesAndSituations/Role"/>
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/Commons/PartiesAndSituations/"/>
