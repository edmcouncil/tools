    </owl:ObjectProperty>
    


    <!-- https://www.omg.org/spec/Commons/ProductsAndServices/produces -->


    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/Commons/ProductsAndServices/produces">
        <rdfs:domain rdf:resource="https://www.omg.org/spec/Commons/ProductsAndServices/Producer"/>
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/Commons/ProductsAndServices/"/>
