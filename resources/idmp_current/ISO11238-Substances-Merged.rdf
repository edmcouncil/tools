        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://www.omg.org/spec/Commons/PartiesAndSituations/playsRole"/>
            <rdf:Description rdf:about="https://www.omg.org/spec/Commons/PartiesAndSituations/actsIn"/>
            <rdf:Description rdf:about="https://www.omg.org/spec/Commons/PartiesAndSituations/hasUndergoer"/>
        </owl:propertyChainAxiom>
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/Commons/PartiesAndSituations/"/>
        <rdfs:label>plays active role that affects</rdfs:label>
        <skos:definition>relates a person or organization to an undergoer they have an impact on under the circumstances</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.omg.org/spec/Commons/PartiesAndSituations/playsActiveRoleThatDirectlyAffects -->


