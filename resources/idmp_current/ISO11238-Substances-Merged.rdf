        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/"/>
        <rdfs:label>has minimum longitude</rdfs:label>
        <skos:definition>a minimum longitude with respect to a range</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- https://www.omg.org/spec/LCC/Countries/CountryRepresentation/hasNumericRegionCode -->


    <owl:DatatypeProperty rdf:about="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/hasNumericRegionCode">
        <rdfs:subPropertyOf rdf:resource="https://www.omg.org/spec/Commons/TextDatatype/hasTextValue"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
