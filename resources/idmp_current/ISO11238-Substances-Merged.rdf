        <rdfs:range rdf:resource="https://www.omg.org/spec/Commons/QuantitiesAndUnits/QuantityDimension"/>
        <rdfs:isDefinedBy rdf:resource="https://www.omg.org/spec/Commons/QuantitiesAndUnits/"/>
        <rdfs:label>has dimension</rdfs:label>
        <skos:definition>indicates a measurable extent associated with a given quantity kind in some system of quantities, which may be derived, depending on the choice of base quantity</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.omg.org/spec/Commons/QuantitiesAndUnits/hasExpression -->


    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/Commons/QuantitiesAndUnits/hasExpression">
