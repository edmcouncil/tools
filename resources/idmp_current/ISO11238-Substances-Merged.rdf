        <rdfs:isDefinedBy rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/META/ChangeManagement/"/>
        <rdfs:label>informative</rdfs:label>
        <skos:definition xml:lang="en">entity that demonstrates how to use some elements of the ontology</skos:definition>
        <AnnotationVocabulary:explanatoryNote xml:lang="en">Informative content includes examples that are useful but should not be incorporated in any application. They have been created to support testing and provide guidance to users with respect to how to implement certain constructs.</AnnotationVocabulary:explanatoryNote>
    </owl:NamedIndividual>
    


    <!-- https://spec.pistoiaalliance.org/idmp/ontology/META/ChangeManagement/Preliminary -->


    <owl:NamedIndividual rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/META/ChangeManagement/Preliminary">
        <rdf:type rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/META/ChangeManagement/MaturityLevel"/>
        <rdfs:isDefinedBy rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/META/ChangeManagement/"/>
        <rdfs:label>preliminary</rdfs:label>
        <skos:definition xml:lang="en">entity that is considered to be in early stages of development</skos:definition>
        <AnnotationVocabulary:explanatoryNote xml:lang="en">Preliminary content is subject to significant change, and may be renamed or moved to another ontology prior to achieving provisional status. Users should be aware that it is not dependable, but could be used for reference and as the basis for further work.</AnnotationVocabulary:explanatoryNote>
    </owl:NamedIndividual>
    


    <!-- https://spec.pistoiaalliance.org/idmp/ontology/META/ChangeManagement/Provisional -->


    <owl:NamedIndividual rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/META/ChangeManagement/Provisional">
        <rdf:type rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/META/ChangeManagement/MaturityLevel"/>
        <rdfs:isDefinedBy rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/META/ChangeManagement/"/>
        <rdfs:label>provisional</rdfs:label>
        <skos:definition xml:lang="en">entity that is considered to be in later stages of development</skos:definition>
        <AnnotationVocabulary:explanatoryNote xml:lang="en">Provisional content may change prior to release but is more stable and reliable than preliminary content.</AnnotationVocabulary:explanatoryNote>
    </owl:NamedIndividual>
    


    <!-- https://spec.pistoiaalliance.org/idmp/ontology/META/ChangeManagement/Release -->


    <owl:NamedIndividual rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/META/ChangeManagement/Release">
